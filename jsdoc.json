{
  "plugins": ["plugins/markdown"],
  "source": {
    "include": ["index.js", "optimalSolution.js"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "template": "templates/default",
    "destination": "./docs/",
    "recurse": true
  }
}
